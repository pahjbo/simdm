<m:mappedModels xmlns:m="http://www.ivoa.net/xml/vodml-binding/v0.9.1">
    <!-- ========================================
    This is a minimal sample file for mapping VO-DML models to XSD or Java using the gradle tooling
     -->

    <model>
        <name>SimDM</name>
        <file>SimDM.vo-dml.xml</file>
        <java-package>org.ivoa.dm.simdm</java-package>
        <xml-targetnamespace prefix="simdb">http://ivoa.net/dm/models/vo-dml/xsd/simdm</xml-targetnamespace>
        <rdb inheritance-strategy="single-table"
             discriminatorColumnLength="64"/>
    </model>
</m:mappedModels>